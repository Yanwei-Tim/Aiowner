<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

    </data>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.dudu.aiowner.utils.customFontUtils.FZLFontTextView
            style="@style/container_operation_tips_text"
            android:text="已发送验证码请输入" />

        <com.dudu.aiowner.utils.customFontUtils.FZLFontEditText
            android:id="@+id/verifyCode_et"
            style="@style/registAndLogin_editText"
            android:background="@null"
            android:layout_marginTop="80dp" />

        <View
            android:id="@+id/regist_phone_view"
            style="@style/edit_blue_line"
            android:layout_below="@id/verifyCode_et" />


        <com.dudu.aiowner.utils.customFontUtils.FZLFontTextView
            style="@style/button_big"
            android:layout_marginTop="170dp"
            android:background="@drawable/btn_bg_sel_line"
            android:onClick="startInitPassword"
            android:text="确 定" />

    </RelativeLayout>

</layout>