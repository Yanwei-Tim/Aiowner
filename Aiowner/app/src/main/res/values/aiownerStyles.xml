<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AiownerBackground" parent="AppTheme">
        <!--<item name="android:background">@color/aiowner</item>-->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppBaseTheme" parent="android:Theme">
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowTitleSize">50dp</item>
        <item name="android:windowTitleBackgroundStyle">@style/AiownerBackground</item>
    </style>

    <style name="container_operation_tips_text">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="registAndLogin_editText">
        <item name="android:layout_width">280dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:gravity">center</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">16sp</item>
        <item name="android:background">@null</item>
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="edit_blue_line">
        <item name="android:layout_width">280dp</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:background">@color/bluecolor</item>
    </style>

    <style name="button_big">
        <item name="android:layout_width">280dp</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:clickable">true</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">22sp</item>
    </style>

    <style name="theft_btn">
        <item name="android:layout_width">152dp</item>
        <item name="android:layout_height">23.5dp</item>
        <item name="android:clickable">true</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="theft_psw_ring">
        <item name="android:layout_width">69dp</item>
        <item name="android:layout_height">69dp</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="theft_psw_ring_white">
        <item name="android:background">@android:color/white</item>
        <item name="android:layout_width">69dp</item>
        <item name="android:layout_height">69dp</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="geture_btn">
        <item name="android:layout_width">120dp</item>
        <item name="android:layout_height">32dp</item>
        <item name="android:clickable">true</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="assistant_btn">
        <item name="android:layout_width">65dp</item>
        <item name="android:layout_height">20.5dp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">11sp</item>
    </style>


    <style name="login_container_tips_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">40dp</item>
        <item name="android:textColor">@color/bluecolor</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="test_Speed_imageview_style1">
        <item name="android:layout_width">10dp</item>
        <item name="android:layout_height">6dp</item>
        <item name="android:layout_marginTop">8dp</item>
    </style>

    <style name="test_Speed_imageview_style2">
        <item name="android:layout_width">9.5dp</item>
        <item name="android:layout_height">16.5dp</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>

    <style name="test_Speed_imageview_style3">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:layout_marginTop">13dp</item>
    </style>

    <style name="test_Speed_FZLFontTextView_style1">
        <item name="android:layout_width">36dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">18dp</item>
        <item name="android:textColor">@color/bluecolor</item>
        <item name="android:textSize">18dp</item>
    </style>

    <style name="test_Speed_FZLFontTextView_style2">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_marginRight">19dp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">18dp</item>
    </style>

    <style name="looting_switch_style">
        <item name="android:layout_width">116dp</item>
        <item name="android:layout_height">75dp</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:background">@drawable/looting_switch_btn_sel</item>
        <item name="android:checked">false</item>
        <item name="android:textOff">""</item>
        <item name="android:textOn">""</item>
    </style>

    <style name="bottomStyle">
        <item name="android:windowEnterAnimation">@anim/bottom_pup_anim_out</item>
        <item name="android:windowExitAnimation">@anim/bottom_pup_anim_in</item>
    </style>

    <!-- activity_pwd.xml -->
    <style name="style_edit">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/edit_bg</item>
    </style>

</resources>



