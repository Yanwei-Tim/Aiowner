<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dudu.aiowner" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AiownerBackground" >
        <activity
            android:name=".ui.main.HomeActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.activity.register.TelephoneNumberActivity" />
        <activity android:name=".ui.activity.register.IdentifyingCodeActivity" />
        <activity android:name=".ui.activity.register.InitPasswordActivity" />
        <activity android:name=".ui.activity.login.LoginActivity" />
        <activity android:name=".ui.base.BaseActivity" />
        <activity android:name=".ui.activity.login.ForgetPasswordActitivy" />
        <activity android:name=".ui.main.MainActivity" />
        <activity android:name=".ui.activity.preventTheft.PreventTheftActivity" >
        </activity>
    </application>

</manifest>
